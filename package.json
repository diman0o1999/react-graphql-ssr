{
	"name": "react-graphql",
	"version": "0.2.0",
	"main": "index.js",
	"private": true,
	"author": "Diman0o1999",
	"scripts": {
		"dev": "next -p 8080",
		"build": "next build",
		"start": "next start",
		"post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
		"prepare": "husky install",
