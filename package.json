		"husky": "6.0.0",
		"prettier": "2.2.1",
		"pretty-quick": "3.1.0",
		"react-live": "2.2.3",
		"typescript": "4.2.4",
		"apollo": "^2.32.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --pattern \"**/*.*(js|jsx|tsx|ts)\" --staged"
		}
