		"react-final-form": "^6.5.3",
		"styled-components": "5.2.3"
	},
	"devDependencies": {
		"@types/node": "15.0.1",
		"@types/react": "17.0.4",
		"@types/react-dom": "17.0.3",
		"@types/react-select": "4.0.15",
		"@types/styled-components": "5.1.9",
